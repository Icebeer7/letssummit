{
  "expo": {
    "name": "journime",
    "slug": "journime",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "reactnativefoundation",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.journime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.journime"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1E1335",
          "dark": { "backgroundColor": "#1E1335" }
        }
      ]
    ],
    "splash": {
      "backgroundColor": "#1E1335"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
